            <kind>1</kind>
            <string>/// disable check
if(mode == 0) {
    var mx, my;
    for(var i = 0; i &lt; 4; i++) {
    if(device_mouse_check_button_released(i, mb_left)) {
        mx = device_mouse_x_to_gui(i);
        my = device_mouse_y_to_gui(i);
        
        if(mx &lt; title_y * 2) and (my &lt; 700) and (my &gt; 600) {
            mode = -1;
            select = -1;
            zoom = 1;
            target_x = 480/2;
            target_y = 270/2;
        }
        
        if(mx &gt; 1280 - title_y * 2) and (my &lt; 700) and (my &gt; 600) {
            mode = 1;
            zoom = 1;
            global.controller = true;
            target.active = true;
        }
    }
    }
}else if (mode == 1) {
    var mx, my;
    for(var i = 0; i &lt; 4; i++) {
        if(device_mouse_check_button_released(i, mb_left)) {
            mx = device_mouse_x_to_gui(i);
            my = device_mouse_y_to_gui(i);
            
            if(mx &lt; 170) and (mx &gt; 10) and (my &gt; 10) and (my &lt; 70) {
                mode = -1;
                select = -1;
                zoom = 1;
                target_x = 480/2;
                target_y = 270/2;
                global.controller = false;
                target.active = false;
                target.image_xscale = 1;
                target.weapon_dir = 0;
                target.x = target.xstart
                target.y = target.ystart
                target = -1
            }
        }
    }
}else if (mode == 2) {
    var mx, my;
    for(var i = 0; i &lt; 4; i++) {
        if(device_mouse_check_button_released(i, mb_left)) {
            mx = device_mouse_x_to_gui(i);
            my = device_mouse_y_to_gui(i);
            
            if(obj_door.matching == false) {
                if(mx &lt; 170) and (mx &gt; 10) and (my &gt; 10) and (my &lt; 70) {
                    obj_choose_view.zoom = 1;
                    obj_choose_view.mode = 1;
                    obj_door.trigger = false;
                    global.controller = true;
                }
                
                if(mx &lt; 1270) and (mx &gt; 1270-250) and (my &gt; 710-100) and (my &lt; 710) {
                    obj_door.matching = true;
                    time_dot = 0;
                    alarm[0] = 50;
                    /// matching start
                    buffer = buf_new(16);
                    buf_write(buffer, buffer_u8, 0);
                    buf_write(buffer, buffer_u8, select);
                    supersocket_send(buffer, signal.matching, sendTo.Server);
                    buf_del(buffer);
                }
            }else {
                if(mx &lt; 170) and (mx &gt; 10) and (my &gt; 10) and (my &lt; 70) {
                    obj_door.matching = false;
                    /// matching end
                    buffer = buf_new(16);
                    buf_write(buffer, buffer_u8, 1);
                    supersocket_send(buffer, signal.matching, sendTo.Server);
                    buf_del(buffer);
                }
            }
        }
    }
}
</string>
