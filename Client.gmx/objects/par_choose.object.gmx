            <string>/// draw
if (z &gt; 0)
    image_index = 14;
    
draw_set_alpha(0.5);
draw_set_color(c_black);
draw_ellipse(x - 10, y - 2, x + 10, y + 2, false);
draw_set_alpha(1);
draw_sprite_ext(sprite_index, image_index, x, y - z, image_xscale, image_yscale, image_angle, c_white, image_alpha);
